#include<stdio.h>
int main(){
    int num[3];
    for (int i=0; i<3; i++){
        scanf("%d",&num[i]);
    }
    
    int temp, swap;

    for (int i=0; i<2; i++){
        swap = 0;
        for (int j=0; j < 2-i; j++){
            if (num[j] < num[j+1]){
                temp = num[j];
                num[j] = num[j+1];
                num[j+1] = temp;
                swap = 1;
            }
        }
        if (swap = 0)
            break;
    }

    printf("%d\n", num[0]);
    return 0;
}
